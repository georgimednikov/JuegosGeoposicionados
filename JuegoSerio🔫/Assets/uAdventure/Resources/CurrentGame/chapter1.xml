      </effect>
    </dialogue-node>
    <dialogue-node nodeindex="5" editor-x="601" editor-y="372">
      <speak-player>
        <resources name="No name" />
        <text>En esta imágen se ve una estatua de un caballo al borde de caerse al suelo.</text>
      </speak-player>
      <condition />
      <child nodeindex="6" />
      <effect>
        <generate-object idTarget="ImagenEstatuaVelazquez" />
