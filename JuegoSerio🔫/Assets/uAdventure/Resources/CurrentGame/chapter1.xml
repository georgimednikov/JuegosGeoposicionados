    <option-node nodeindex="4" preListening="yes" editor-x="1777" editor-y="334" x="1777" y="334">
      <speak-player>
        <text>¿Libros?</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="5" />
      <speak-player>
        <text>¿Hogar del combatiente?</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="6" />
      <speak-player>
        <text>¿Te suena esta foto?</text>
