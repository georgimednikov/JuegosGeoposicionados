    <option-node nodeindex="1" preListening="yes" editor-x="15046" editor-y="92" x="15046" y="92">
      <speak-player>
        <text>¿Qué pasó aquí?</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="2" />
      <speak-player>
        <text>¿Quién eres?</text>
        <resources name="No name" />
