          <activate flag="IntroductionFinished" />
          <condition />
          <trigger-scene idTarget="MapaJugable" x="539" y="455" transitionTime="1000" transitionType="5" />
          <condition />
        </effect>
      </end-conversation>
    </dialogue-node>
    <dialogue-node nodeindex="3" editor-x="616" editor-y="86">
      <speak-player>
        <resources name="No name" />
        <text>Primero, una foto del frente de la Ciudad Universitaria.</text>
      </speak-player>
      <condition />
      <child nodeindex="4" />
      <effect>
        <generate-object idTarget="MapaFrente" />
        <condition />
      </effect>
    </dialogue-node>
    <dialogue-node nodeindex="4" editor-x="886" editor-y="87">
      <speak-player>
        <resources name="No name" />
        <text>Después, una fotografía de lo que parece un pabellón lleno de escombros.</text>
      </speak-player>
      <condition />
      <child nodeindex="5" />
      <effect>
        <generate-object idTarget="ImagenPabellonFilosofia" />
